<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nikola.jakshic.dagger">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.nikola.jakshic.dagger.DaggerApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_dagger"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_dagger_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.nikola.jakshic.dagger.view.activity.PlayerActivity"
            android:theme="@style/DetailTheme" />
        <activity android:name="com.nikola.jakshic.dagger.view.activity.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.nikola.jakshic.dagger.view.activity.SearchActivity"
            android:theme="@style/SearchTheme" />
        <activity android:name="com.nikola.jakshic.dagger.view.activity.MatchActivity" />
    </application>
</manifest>
